<div class="container">
		<h1>Sign Up</h1>
	<div class="big">
		<%= form_for @user do |f| %>
			<div class="auth"><%= f.text_field :first_name, placeholder: "First Name "%></div>
			<div class="auth"><%= f.text_field :last_name, placeholder: "Last Name "%></div>
			<div class="auth"><%= f.text_field :email, placeholder: "Email here" %></div>
			<div class="auth"><%= f.password_field :password, placeholder: "Password here"%></div>
			<div class="auth" id="submit"><%= f.submit %></div>
		<% end %>
	</div>
		</div>